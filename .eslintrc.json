{
  "env": {
    "browser": true,
    "es2020": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "fullstack"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "root": true,
  "plugins": ["react", "jest", "react-redux"],
  "rules": {
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/prefer-to-have-length": "warn",
    "jest/valid-expect": "error"
  }
}
